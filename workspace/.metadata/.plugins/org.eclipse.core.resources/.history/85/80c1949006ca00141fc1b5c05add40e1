		for(int i=0; i<3; i++){
			m 	   = Input.getInt();
			n  	   = Input.getInt();
			int mn = n % m;
			System.out.println(mn);
		}
		System.out.println("end");
		
		
		//P5-2-1
		int	[] x1 = {102,98,122,-97,88};
		for(int i=0; i<5; i++){
			System.out.println(x1[i]);
		}
		
		
		//P5-2-2
	    double [] x2 = {12.5,33.5,-12.7,18.4,13.33};
	    for(int i=0; i<5; i++){
	    	System.out.println(x2[i]);
	    }
	    
	    
