	    String [] x3 = {"赤","青","黄","白","黒"};
	    for(int i=0; i<5; i++){
	    	System.out.println(x3[i]);
	    }
		
	
		//P5-3
	    int [] y1 = {10, 23, 13, 7, 22};
	    int [] y2 = {2, 3, 5, 3, 4};
	    for(int i=0; i<5; i++){
	    	int y3 = y1[i] + y2[i];
	    	System.out.println(y1[i] + "+" + y2[i] + "=" + y3);
	    }
		
		
	    /*
	     * P5-4-1	B	C
	     *  
	     * P5-4-2	D 	E
	     * 		コンパイルエラーと実行時エラーの区別ができていなかった
	     */
	    
	    //P5-4-1 実証しても結果が10で意味不明…
	    int t = 0;
	    for(int i=0; i<5; i++)
	    	t+=i; System.out.println(t);
	    
	    		
		
		
		
		
		
		
		
		
