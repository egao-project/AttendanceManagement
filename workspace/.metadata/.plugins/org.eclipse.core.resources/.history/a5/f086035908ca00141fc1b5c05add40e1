package lib;

public class Test6 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

		
		//	第6章　通過テスト
		
		
		//	1
		
		double [] a1 	= {12.3, 13.5, 12.2, 13.0, 12.8, 12.3};
		double totalA1	= 0;
		for(int i=0; i<a1.length; i++){
			totalA1	+=	a1.length;
		}
		double meanA1	=	totalA1 / a1.length;
		System.out.println("合計＝"+	totalA1);
		System.out.println("平均＝"+	meanA1);
		
		for(double aa : a1){
			System.out.print(aa+ " ");
		}
	}

}
